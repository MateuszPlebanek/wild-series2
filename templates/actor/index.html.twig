{% extends 'base.html.twig' %}
{% block title %}Actors{% endblock %}
{% block body %}
  <h1>Actors</h1>
  <ul>
    {% for actor in actors %}
      <li><a href="{{ path('actor_show', { id: actor.id }) }}">{{ actor.name }}</a></li>
    {% else %}
      <li>No actors yet.</li>
    {% endfor %}
  </ul>
{% endblock %}
