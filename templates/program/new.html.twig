{% extends 'base.html.twig' %}

{% block title %}New Program{% endblock %}

{% block body %}
  <h1>Create a new Program</h1>

  {{ form_start(form, { attr: { novalidate: 'novalidate' } }) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    <button type="submit">Add program</button>
  {{ form_end(form) }}

  <p><a href="{{ path('program_index') }}">‚Üê Back to programs</a></p>
{% endblock %}
